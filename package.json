{
  "name": "domwires-devkit",
  "version": "0.9.82",
  "description": "Devkit based on DomWires Framework",
  "repository": {
    "type": "git",
    "url": "https://github.com/CrazyFlasher/domwires-devkit-ts"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/index.common.js",
  "main:client": "./dist/index.client.js",
  "main:server": "./dist/index.server.js",
  "dependencies": {
    "domwires": "0.9.88",
    "@types/express": "4.17.13",
    "axios": "0.27.2",
    "cors": "2.8.5",
    "express": "4.18.1",
    "minimist": "1.2.6",
    "mongodb": "4.9.0",
    "socket.io": "4.5.2",
    "socket.io-client": "4.5.2"
  },
  "devDependencies": {
    "@types/chai": "4.3.3",
    "@types/mocha": "9.1.1",
    "@types/node": "18.7.12",
    "@typescript-eslint/eslint-plugin": "5.34.0",
    "@typescript-eslint/parser": "5.34.0",
    "chai": "4.3.6",
    "eslint": "8.22.0",
    "mocha": "10.0.0",
    "ts-node": "10.9.1",
    "typescript": "4.7.4",
    "rimraf": "3.0.2",
    "copyfiles": "2.4.1"
  },
  "engines": {
    "node": ">=16.17.0"
  },
  "scripts": {
    "model_gen": "ts-node scripts/model_from_type_gen.ts --in=/test/mock/typesForModels --templatesPath=/res --overwrite --verbose --relatedImportPath=../",
    "test": "mocha -r ts-node/register test/*.ts --exit --timeout=2000",
    "eslint-setup": "npm install eslint --save-dev",
    "eslint-run": "eslint . --ext .ts",
    "clean": "rimraf dist/",
    "copy-files": "copyfiles -u 1 src/**/*.html src/**/*.css dist/"
  }
}
